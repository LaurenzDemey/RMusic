{
  "name": "rmusic",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/RMusic/stats.json",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:html && npm run lint:scss",
    "lint-parallel": "npm-run-all --parallel lint:*",
    "lint-fix": "npm run lint:ts -- --fix && npm run lint:scss -- --fix",
    "lint:html": "htmlhint --config .htmlhintrc ./src/**/*.{htm,html}",
    "lint:ts": "tslint -p tsconfig.json",
    "lint:scss": "stylelint ./src/**/*.scss",
    "prettier": "prettier ./src/**/*.{ts,scss} --write",
    "fake-api": "json-server --watch fake-db/db.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-regular-svg-icons": "5.7.2",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "htmlhint": "^0.10.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.2",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "protractor": "~5.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
